<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="ScrapMapper">
  
  <!-- sql문 작성 -->
  
  <insert id="create">
  insert into tbl_scrap (Scrap_ID,Member_ID,Code_ID,Contents_ID,Contents_Title,Contents_Photo)
  value(#{scrapID},#{memberID},#{codeID},#{contentsID},#{contentsTitle},#{contentsPhoto})
  </insert>
  
  </mapper>
  